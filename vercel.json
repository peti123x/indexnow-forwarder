{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.2"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",
  "env": {
      "SHOPIFY_SIGNATURE": "@SHOPIFY_SIGNATURE",
      "BING_KEY": "@BING_KEY",
      "DOMAIN": "@DOMAIN"
  }
}